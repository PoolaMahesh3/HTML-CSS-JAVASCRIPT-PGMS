function EvenOddSums(a){
    var sum=0;
    var sum1=0;
    for(let i=0; i<a.length; i++)
    {
        if(a[i]%2==0)
        {
            sum=sum+a[i];
        
        }
        else{
            sum1=sum1+a[i];

        }
    }
    
}

let arr=[1,2,3,4,5,6]
let n =arr.length;

let a=EvenOddSums(arr,n);
console.log(a);
